@model List<MovieModel>

<h2>Movies</h2>

<table class="table">
    <tr>
        <th>Title</th>
        <th>Year</th>
        <th>Poster</th>
    </tr>

    @foreach (var movie in Model)
    {
<tr>
    <td><a href=/Movie/ViewMovie/@movie.Title>@movie.Title</a></td>
    <td>@movie.Year</td>
    <td><img src="@movie.Poster" alt="@movie.Title"></td>
</tr>}
</table>